.tab-pill.active { background: #2563EB; color: white; border-color: #2563EB; font-weight: 600; }

.bg-glass { background: rgba(249,250,251,0.9); backdrop-filter: blur(8px); }
.toggle-wrap { background: #E5E7EB; padding: 6rpx; border-radius: 16rpx; display: flex; }
.toggle-btn { flex: 1; text-align: center; padding: 12rpx; font-size: 26rpx; font-weight: 700; color: #6B7280; border-radius: 12rpx; }
.on-blue { background: white; color: #2563EB; box-shadow: 0 2rpx 4rpx rgba(0,0,0,0.05); }
.on-pink { background: white; color: #DB2777; box-shadow: 0 2rpx 4rpx rgba(0,0,0,0.05); }

/* Rental Row */
