/* Global */
page { background: #F9FAFB; font-family: -apple-system, sans-serif; color: #1F2937; height: 100%; display: flex; flex-direction: column; }
.container { height: 100vh; display: flex; flex-direction: column; overflow: hidden; }
image { display: block; }
button { margin: 0; padding: 0; background: none; line-height: normal; }
button::after { border: none; }

/* Custom Header (Fixed) */
.custom-nav { position: fixed; top: 0; left: 0; width: 100%; background: linear-gradient(to right, #2563EB, #1D4ED8); z-index: 100; box-shadow: 0 4rpx 10rpx rgba(37,99,235,0.2); box-sizing: border-box; }
.nav-body { display: flex; align-items: center; padding-left: 24rpx; width: 100%; box-sizing: border-box; }
.nav-left { display: flex; align-items: center; flex-shrink: 0; margin-right: 24rpx; }
.logo-box { background: rgba(255,255,255,0.2); border-radius: 50%; padding: 8rpx; border: 2rpx solid rgba(255,255,255,0.3); }
.app-name { color: white; font-weight: 700; font-size: 34rpx; margin-left: 12rpx; letter-spacing: 2rpx; }
.search-wrapper { flex: 1; background: rgba(255,255,255,0.15); border-radius: 32rpx; height: 64rpx; display: flex; align-items: center; padding: 0 20rpx; position: relative; margin-right: 180rpx; } 
.search-icon { width: 32rpx; height: 32rpx; opacity: 0.7; margin-right: 12rpx; }
.search-inp { flex: 1; font-size: 26rpx; color: white; height: 100%; }
.ph-style { color: rgba(255,255,255,0.6); }
.clear-btn { padding: 8rpx; opacity: 0.8; }

/* Content */
.main-scroll { flex: 1; overflow-y: auto; background: #F9FAFB; }
.pb-safe-area { padding-bottom: 200rpx; }

/* Banner */
.banner { height: 360rpx; background: linear-gradient(135deg, #6366F1, #3B82F6, #60A5FA); border-radius: 32rpx; position: relative; overflow: hidden; color: white; }
.banner-inner { position: relative; z-index: 10; height: 100%; display: flex; flex-direction: column; align-items: center; justify-content: center; }
.banner-t1 { font-size: 44rpx; font-weight: 700; margin-bottom: 12rpx; text-shadow: 0 2rpx 4rpx rgba(0,0,0,0.1); }
.banner-t2 { font-size: 26rpx; opacity: 0.9; margin-bottom: 32rpx; }
.highlight { font-weight: 700; color: #FDE047; }
.btn-pill { background: white; color: #2563EB; font-size: 26rpx; font-weight: 700; padding: 12rpx 40rpx; border-radius: 999rpx; box-shadow: 0 4rpx 8rpx rgba(0,0,0,0.1); }
.dec-circle { position: absolute; border-radius: 50%; background: rgba(255,255,255,0.1); filter: blur(40px); }
.c1 { width: 300rpx; height: 300rpx; top: -100rpx; right: -100rpx; }
.c2 { width: 200rpx; height: 200rpx; bottom: 50rpx; left: -50rpx; }

/* Grid Entries */
.grid-4 { display: grid; grid-template-columns: repeat(4, 1fr); gap: 16rpx; }
.entry-btn { display: flex; flex-direction: column; align-items: center; font-size: 24rpx; color: #4B5563; font-weight: 500; }
.entry-btn:active { opacity: 0.6; }
.icon-box { padding: 20rpx; border-radius: 28rpx; margin-bottom: 12rpx; display: flex; align-items: center; justify-content: center; }
.bg-blue-50 { background: #EFF6FF; color: #2563EB; }
.bg-orange-50 { background: #FFF7ED; color: #EA580C; }
.bg-purple-50 { background: #FAF5FF; color: #9333EA; }
.bg-pink-50 { background: #FDF2F8; color: #DB2777; }

/* Sections */
.v-bar { width: 8rpx; height: 32rpx; background: #2563EB; border-radius: 8rpx; margin-right: 16rpx; }
.title-lg { font-size: 32rpx; font-weight: 700; color: #111827; }
.more-link { font-size: 24rpx; color: #9CA3AF; display: flex; align-items: center; }

/* Cards */
.grid-2 { display: grid; grid-template-columns: 1fr 1fr; gap: 24rpx; }
.card { background: white; border-radius: 24rpx; overflow: hidden; border: 2rpx solid #F3F4F6; display: flex; flex-direction: column; height: 100%; transition: transform 0.1s; }
.card:active { transform: scale(0.98); }
.card-top { height: 240rpx; background: #E5E7EB; position: relative; }
.cover-img { width: 100%; height: 100%; }
.tag-row { position: absolute; top: 12rpx; left: 12rpx; display: flex; flex-wrap: wrap; gap: 8rpx; }
.tag { background: rgba(0,0,0,0.6); backdrop-filter: blur(4px); color: white; font-size: 20rpx; padding: 4rpx 12rpx; border-radius: 8rpx; }
.card-bot { padding: 20rpx; flex: 1; display: flex; flex-direction: column; }
.c-title { font-size: 28rpx; font-weight: 700; color: #1F2937; margin-bottom: 6rpx; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.c-desc { font-size: 22rpx; color: #6B7280; line-height: 1.4; margin-bottom: 12rpx; flex: 1; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden; }
.add-btn { background: #EFF6FF; padding: 8rpx; border-radius: 12rpx; color: #2563EB; }

/* Adv Card */
.adv-card { background: linear-gradient(135deg, #FFFFFF, #EFF6FF); border-radius: 32rpx; padding: 32rpx; border: 2rpx solid #DBEAFE; }
.adv-head { font-size: 30rpx; font-weight: 700; text-align: center; margin-bottom: 24rpx; display: block; color: #1F2937; }
.adv-items { display: flex; flex-direction: column; gap: 24rpx; }
.adv-row { display: flex; align-items: flex-start; gap: 24rpx; }
.adv-t1 { font-size: 26rpx; font-weight: 700; color: #1F2937; display: block; }
.adv-t2 { font-size: 22rpx; color: #6B7280; line-height: 1.4; display: block; margin-top: 4rpx; }

/* Tabs & Filter (‰øÆÂ§çÔºöÂ∞èÂ∑ßÁ≤æËá¥) */
.sticky-sub { position: sticky; z-index: 99; background: rgba(249,250,251,0.95); backdrop-filter: blur(8px); }
.tab-scroll { white-space: nowrap; padding: 20rpx 32rpx; border-bottom: 2rpx solid #E5E7EB; display: flex; }
/* ÂÖ≥ÈîÆ‰øÆÊîπÔºöpaddingÂáèÂ∞èÔºåfont-sizeÂáèÂ∞èÔºåline-heightÂØπÈΩê */
